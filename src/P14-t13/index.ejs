<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="cache-control" content="max-age=1800">
    <title></title>
    <script src="js/sdk.js?v=<%=new Date().getTime()%>"></script>
    <script src="js/fit.js?v=<%=new Date().getTime()%>"></script>
    <link rel="stylesheet" href="css/index.css?v=<%=new Date().getTime()%>">
</head>
<body>
<div class="container" id="" data-bind-updateuser="1">

    <section class="commom">
        <div class="sec-desc">
           <div class="desc hide"></div> 
        </div>
        <div class="title hide">
            <h3></h3>
        </div> 
    </section>
    
    <section class="stu hide">

        <div class="sec-middle flex-box">
            <div class="content flex-box">
                <img src="" alt="" class="theme-pic hide shadow">
                <div class="btn-audio hide" data-syncactions="btn-audio-1">
                    <img src="image/btn-audio.png" alt="">
                </div>
            </div>
        </div>
        
        <div class="avatars">
            <ul>
                <!-- <li>
                    <img src="image/avatars.png" alt="" class="avatar">
                    <p class="name">stu1</p>
                    <div class="absence"></div>
                </li> -->
            </ul>
        </div>

        <div class="bomb"  data-syncactions="bomb-1">
            <div class="bomb-img">
                <img src="image/bomb.png" alt="">
            </div>
            <p class="count-down" data-syncactions="countdown-1"></p>
            <img src="image/bomb.gif" alt="" class="bomb-gif hide">
        </div>

    </section>
    
    <section class="tea hide">
        <div id="tg"></div>
        <div class="sec-middle flex-box">
            <div class="content flex-box">
                <img src="" alt="" class="theme-pic hide">
                <div class="btn-audio hide" data-syncactions="btn-audio-1">
                    <img src="image/btn-audio.png" alt="">
                </div>
            </div>
        </div>
        <div class="avatars">
            <ul></ul>
        </div>
        <div class="bomb"  data-syncactions="bomb-1">
            <div class="bomb-img">
                <img src="image/bomb.png" alt="">
            </div>
            <p class="count-down" data-syncactions="countdown-1"></p>
            <div class="btn start"></div>
            <img src="image/bomb.gif" alt="" class="bomb-gif hide">
        </div>
    </section>
    
    <audio src="" id="themeAudio"></audio>
    <audio src="" id="bombAudio"></audio>
</div>
<script type="text/javascript" src="content.js?v=<%=new Date().getTime()%>"></script>
<script type="text/javascript" src="js/index.js?v=<%=new Date().getTime()%>"></script>
</body>
</html>